
SELECT *
FROM customers 
WHERE last_name REGEXP 'field$|mac|rose'


SELECT *
FROM customers 
WHERE last_name REGEXP 'f$|m|r'


SELECT *
FROM customers 
WHERE last_name REGEXP '^mac|bru'


SELECT *
FROM customers 
WHERE last_name REGEXP '[gim]e'


SELECT *
FROM customers 
WHERE last_name REGEXP '[ie]e'


SELECT *
FROM customers 
WHERE last_name REGEXP '[a-h]e'


SELECT *
FROM customers 
WHERE last_name REGEXP '[abcd]e'


SELECT *
FROM customers 
WHERE last_name REGEXP '[f-j]e'


SELECT *
FROM customers 
WHERE first_name regexp 'ELKA|AMBUR'


SELECT *
FROM customers 
WHERE last_name regexp 'EY$|ON'



SELECT *
FROM customers 
WHERE last_name regexp 'MY$|SE'



SELECT *
FROM customers 
WHERE last_name regexp '^MY|SE'



SELECT *
FROM customers 
WHERE last_name regexp '[B]|R|U'



